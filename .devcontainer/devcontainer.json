{
	"name": "graphql-client-boilerplate",
	"dockerComposeFile": "docker-compose.yml",
	"service": "client",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"orsenkucher.vscode-graphql",
				"stylelint.vscode-stylelint"
			]
		}
	},
	"forwardPorts": [
		3000
	]
}